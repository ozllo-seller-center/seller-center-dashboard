{
    "extends": "tslint:recommended",
    "defaultSeverity": "error",
    "rulesDirectory": [],
    "rules": {
        "max-line-length": {
            "options": [
                140
            ]
        },
        "semicolon": [
            false,
            "always",
            "ignore-interfaces"
        ],
        "quotemark": [
            true,
            "single",
            "avoid-escape"
        ],
        "new-parens": true,
        "no-arg": true,
        "no-bitwise": true,
        "no-conditional-assignment": true,
        "no-consecutive-blank-lines": false,
        "no-console": false
        // "no-console": {
        //     "severity": "warning",
        //     "options": [
        //         "debug",
        //         "info",
        //         "log",
        //         "time",
        //         "timeEnd",
        //         "trace"
        //     ]
        // }
    }
}